<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideControl">
        <attr name="sliderSize" format="integer" />
        <attr name="anzahlPositionen" format="integer" />
        <attr name="sliderColor" format="color" />
        <attr name="controlerColor" format="color" />
        <attr name="sliderOrientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
</resources>